{
  "name": "tailspin",
  "version": "0.0.0",
  "license": "MIT",
  "author": "",
  "description": "",
  "babelMacros": {
    "twin": {
      "preset": "emotion"
    }
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "jest --config jest.config.js",
    "test:changed": "jest --config jest.config.js -o",
    "test:watch": "yarn test --watch",
    "make": "plop",
    "atom": "yarn plop component atom",
    "molecule": "yarn plop component molecule",
    "organism": "yarn plop component organism",
    "wrapper": "yarn plop component wrapper",
    "hook": "yarn plop hook",
    "page": "yarn plop page",
    "context": "yarn plop context",
    "prepare": "husky install",
    "format": "prettier --write './{src,templates}/**/*'",
    "lint": "eslint '{src,templates}/**/*.{js,jsx,ts,tsx}'",
    "lint-fix": "yarn lint --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/css": "^11.5.0",
    "@emotion/react": "^11.5.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "framer-motion": "^6.2.8",
    "next": "latest",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@emotion/babel-plugin": "^11.3.0",
    "@netlify/plugin-nextjs": "^4.3.1",
    "@next/eslint-plugin-next": "^12.1.4",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.34",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-macros": "^3.1.0",
    "chalk": "^5.0.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-config-next": "^12.1.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint": "^8.12.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": ">=12",
    "plop": "^3.0.5",
    "prettier": "^2.3.1",
    "tailwindcss": "^2.2.19",
    "twin.macro": "^2.8.1",
    "typescript": "^4.4.4"
  },
   "resolutions": {
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14"
  },
  "lint-staged": {
    "**/*.(js|ts|tsx)": [
      "eslint --fix"
    ],
    "**/*.(js|json|ts|tsx)": [
      "prettier --write"
    ]
  }
}
